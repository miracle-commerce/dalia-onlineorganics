<div class="priceList-button__wrapper">
  <button id="generate-pdf" class="getPriceListButton">{{'pricelist_pdf.generate_pdf' | t}}</button>
</div>

{% for block in section.blocks %}
  {%- assign sort_collection = collections[block.settings.sort_collection] -%}
    {% paginate sort_collection.products by sort_collection.all_products_count %}
      <script type="application/json" data-collection-products data-collection-title="{{sort_collection.title }}">
        [
          {%- for product in sort_collection.products -%}
            {%- unless product.type == 'Gift Cards' -%}
              { 
                "available": {{product.available | json}},
                "title" : {{product.title | json}},
                "price" : "{{product.price_min | money}}",
                "url" : {{product.url | prepend: request.origin | json}},
                "featured_image": {{product.featured_image | json }},
                "variants": [
                  {%- for variant in product.variants -%}
                    {
                      "available": {{variant.available | json}},
                      "title" : "{{variant.title | replace: '￠', '¢'}}", 
                      "sku": {{variant.sku | json}}, 
                      "barcode": {{variant.barcode | json}},
                      "price" : "{{variant.price | money }}",
                      {%- if variant.compare_at_price > variant.price -%}
                      "compare_at_price" : {{variant.compare_at_price | money | json}},
                      {%- endif -%}
                      "unit_price": {%- if variant.unit_price_measurement.reference_value == 1 -%}{{variant.unit_price | money | append: '/' | append: variant.unit_price_measurement.reference_unit | json}},{%- else -%}{{variant.unit_price | money | append: '/' | append: variant.unit_price_measurement.reference_value | append: variant.unit_price_measurement.reference_unit | json}},{%- endif -%}
                      "featured_image": {{variant.image | json}}, 
                      "weight": {{variant.weight | weight_with_unit | json}}
                    }{%- unless forloop.last -%},{%- endunless -%}
                  {%- endfor -%}
                ]
              }{%- unless forloop.last -%},{%- endunless -%}
            {%- endunless -%}
          {%- endfor -%}
        ]
      </script>
    {% endpaginate %}
{% endfor %}

<script src="{{'pdfmake.min.js' | asset_url }}" ></script>
<script src="{{'vfs_fonts.js' | asset_url }}" ></script>

<script>  
  let collectionsDataEl = document.querySelectorAll("[data-collection-products]"); 
  if(collectionsDataEl.length > 0 ){
    let PDFDefinition = {
      images: {
        logo: "data:image/jpeg;base64,",
      },
      header: {
        margin: [40, 20, 40, 0],
        columns:[
          {
            alignment: 'left',
            image: 'logo',
            // fit: ['auto', 40],
            width: 210,
            link: 'https://onlineorganics.ca/'
          },
          {
            margin: [0, 40, 0, 0], 
            alignment: 'right',
            width: '*',
            stack: [
              {
                text: "{{'pricelist_pdf.phone' | t}}: {{section.settings.phone}}",
                link: "tel: 1-514-928-3663"
              },
              {
                text: "{{'pricelist_pdf.email' | t}}: {{shop.email}}",
                link : "https://onlineorganics.ca/pages/contact-us"              
              }
            ]
          }
        ]
      },
      pageMargins: [ 40, 90, 40, 60 ]
    };
    PDFDefinition.content = [];
    collectionsDataEl.forEach((collectionDataEl)=>{
      PDFDefinition.content.push({text: collectionDataEl.dataset.collectionTitle, fontSize: 15, bold: true, margin: [0, 10, 0, 10], alignment: 'center'})
      let moneyFormat = collectionDataEl.dataset.moneyFormat;
      let collectionProducts = JSON.parse(collectionDataEl.innerText);
      let collectionTable = {layout: 'lightHorizontalLines', margin: [0, 0, 0, 20]};
      collectionTable.table = {
        dontBreakRows: true
      };
      collectionTable.table.widths = ['50%', '20%', '10%','10%','10%'];
      collectionTable.table.headerRows = 1; 
      collectionTable.table.body = [[{text: "{{'pricelist_pdf.product' | t}}", border: [false, true, false, true], fillColor: '#ccc', alignment: 'center'}, {text: 'UPC', border: [false, true, false, true], fillColor: '#ccc', alignment: 'center'}, {text: "{{'pricelist_pdf.weight' | t}}", border: [false, true, false, true], fillColor: '#ccc', alignment: 'center'}, {text:"{{'pricelist_pdf.price' | t}}", border: [false, true, false, true], fillColor: '#ccc', alignment: 'center'}, {text: '$ kg/L', border: [false, true, false, true], fillColor: '#ccc', alignment: 'center'}]];  
      if(collectionProducts.length > 0){
        collectionProducts.forEach((product, index)=>{
          if(product.available){
            if(product.variants.length == 1 && product.variants[0].title == 'Default Title'){
              let title = product.title;
              let upc = product.variants[0].barcode?product.variants[0].barcode: ' '; 
              let weight = product.variants[0].weight? product.variants[0].weight: ' ';
              let price = product.variants[0].price; 
              let unit_price = product.variants[0].unit_price? product.variants[0].unit_price: '';
              let url = '';
              collectionTable.table.body.push([{text: title, link: product.url, bold: true}, upc, weight, price, unit_price]);
            } else{
              collectionTable.table.body.push([{text: product.title, link: product.url, bold: true, colSpan: 5}]); 
              let variantsEl = {colSpan: 5, layout: 'noBorders', fontSize: 8};
              variantsEl.table = {dontBreakRows: true };
              variantsEl.table.widths = ['50%', '20%', '10%','10%','10%'];
              variantsEl.table.body = [];
              product.variants.forEach((variant) => {
                if(variant.available){
                  let title = variant.title; 
                  let upc = variant.barcode ? variant.barcode: ' ';
                  let weight = variant.weight?variant.weight: ' '; 
                  let price = variant.price, moneyFormat; 
                  let unit_price = variant.unit_price? variant.unit_price: '';
                  let url = '';
                  variantsEl.table.body.push([{text: title, margin:[15, 0, 0, 0]}, upc, weight, price, unit_price]);
                }
              });
  
              collectionTable.table.body.push([variantsEl]);
            }
          }
        })
      }
      PDFDefinition.content.push(collectionTable);
    })

    PDFDefinition.content.push({
      alignment: 'center',
      bold: true,
      margin: [0, 20, 0, 0],
      stack: [
        "{{'pricelist_pdf.cant_find_text' | t | replace: '&#39;', "'" }}", 
        "{{'pricelist_pdf.contact_text' | t | replace: '&#39;', "'" }}"
      ]
    })


    // Add PageNumber to the page
    PDFDefinition.footer = (currentPage, pageCount, pageSize)=>{
      var t = {
        layout: "noBorders",
        fontSize: 8,
        margin: [40, 10, 40, 10],
        columns: [
          { width: '50%', alignment: 'left', text: `Page ${currentPage.toString()} {{'pricelist_pdf.of' | t}} ${pageCount}` },
          { width: '50%', alignment: 'right', text: `{{'pricelist_pdf.updated' | t}}: ${new Date().getFullYear()}-${(new Date().getMonth() + 1).toString().padStart(2, '0')}-${(new Date().getDate()).toString().padStart(2, '0')}`}
        ]
      };

      return t;
    }

    // Defination styles 
    PDFDefinition.defaultStyle = {
      fontSize: 10
    }

    console.log(PDFDefinition);
    document.getElementById("generate-pdf").addEventListener('click', ()=>{
      let PDFFileName = `OnlineOrganicsPriceList-${(new Date().toString()).replace(/ \w+-\d+ \(.*\)$/,"")}`;
      if(PDFDefinition){
        pdfMake.createPdf(PDFDefinition).download(PDFFileName);      
      }
    })
  }
</script>



